package SeleniumtestExample;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class My_first_script {

	public static void main(String[] args) {

		System.setProperty("webdriver.chromedriver", "C:\\Selenium\\InstallationStuff\\ExeFiles_Browser Drivers\\chromedriver74.0.3729.6_win32.exe");
		WebDriver driver = new ChromeDriver();
		
		driver.get("https://google.com.mx");
	}

}
